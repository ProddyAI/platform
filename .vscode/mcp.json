{
	"servers": {
		"context7": {
			"type": "http",
			"url": "${env:CONTEXT7_MCP_URL}",
			"headers": {
				"CONTEXT7_API_KEY": "${env:CONTEXT7_API_KEY}"
			}
		},
		"sequential-thinking": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			]
		},
		"convex-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"convex@latest",
				"mcp",
				"start"
			]
		},
		"memory": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory"
			]
		},
		"deepwiki": {
			"command": "npx",
			"args": [
				"-y",
				"mcp-remote",
				"https://mcp.deepwiki.com/sse"
			],
			"env": {}
		}
	}
}